	"configurations": [
		{
			"type": "cppvsdbg",
			"program": "${workspaceRoot}/target/debug/tetra.exe",
			"stopAtEntry": false,
			"environment": [],
			"request": "launch",
			"name": "Debug executable 'tetra'",
			"cargo": {
				"args": [
					"build",
					"--bin=tetra",
					"--package=tetra"
				],
				"filter": {
					"name": "tetra",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}",
			"console": "externalTerminal"
		},
